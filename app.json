{
      "expo": {
            "name": "Almana",
            "slug": "almana-calendar",
            "privacy": "public",
            "sdkVersion": "33.0.0",
            "platforms": ["ios", "android", "web"],
            "version": "1.0.0",
            "orientation": "portrait",
            "icon": "./assets/icon-app.png",
            "splash": {
                  "image": "./assets/splash.png",
                  "resizeMode": "contain",
                  "backgroundColor": "#ffffff"
            },
            "updates": {
                  "fallbackToCacheTimeout": 0
            },
            "assetBundlePatterns": ["**/*"],
            "ios": {
                  "bundleIdentifier": "com.alma.almana",
                  "supportsTablet": true
            },
            "android": {
                  "package": "com.alma.almana"
            }
      }
}
